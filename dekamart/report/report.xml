<odoo>

        <!-- BERKORELASI DI DAFTARSUPPLIEREXCEL.PY UTK MENJALANKAN SCIRPTNYA -->
        <!-- SINTAKS INI UTK MEMUNCULKAN BOTTOM PRINT DI SUPLIER -->
        <record id="report_supplier_xlsx" model="ir.actions.report">
            <field name="name">Supplier Excel</field>
            <field name="model">dekamart.supplier</field>
            <field name="report_type">xlsx</field>
            <field name="report_name">dekamart.report_supplier_xlsx</field>
            <field name="report_file">dekamart.report_supplier_xlsx</field>
            <field name="binding_model_id" ref="model_dekamart_supplier"/>
            <field name="binding_type">report</field>
        </record>
        

        <record id="report_penjualan_xlsx" model="ir.actions.report">
            <field name="name">Supplier Excel</field>
            <field name="model">dekamart.penjualan</field>
            <field name="report_type">xlsx</field>
            <field name="report_name">dekamart.report_penjualan_xlsx</field>
            <field name="report_file">dekamart.report_penjualan_xlsx</field>
            <field name="binding_model_id" ref="model_dekamart_penjualan"/>
            <field name="binding_type">report</field>
        </record>

        <!-- SINTAKS INI UTK MEMUNCULKAN BOTTOM PRINT DI PENJUALAN -->
        <record id="report_penjualan_pdf" model="ir.actions.report">
            <field name="name">Faktur PDF</field>
            <field name="model">dekamart.penjualan</field>
            <field name="report_type">qweb-pdf</field>
            <field name="report_name">dekamart.report_penjualan_pdf</field>
            <field name="report_file">dekamart.report_penjualan_pdf</field>
            <field name="binding_model_id" ref="model_dekamart_penjualan"/>
            <field name="binding_type">report</field>
        </record>

</odoo>
